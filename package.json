{
	"name": "vscode-dbf",
	"displayName": "vscode-dbf",
	"description": "DBF viewer for VSCode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:dbf-table",
		"onCommand:vscode-dbf.helloWorld"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-dbf.helloWorld",
				"title": "Hello World"
			}
		],
		"customEditors": [
			{
				"viewType": "dbf-table",
				"displayName": "DBF Table",
				"selector": [
					{
						"filenamePattern": "*.dbf"
					}
				]
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
